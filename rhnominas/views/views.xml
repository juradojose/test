    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="rhnominas.list1">
      <field name="name">nominas list</field>
      <field name="model">rhnominas.rhnominas</field>
      <field name="arch" type="xml">
        <tree>
         <field name="clave"/>
         <field name="dependencia"/>
         <field name="nivel"/>
        </tree>
      </field>
    </record>

    <!-- explicit form view definition -->
    <record model="ir.ui.view" id="rhnominas.form1">
      <field name="name">nominas form</field>
      <field name="model">rhnominas.rhnominas</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <field name="clave"/>
              <field name="dependencia"/>
              <field name="nivel"/>
              <field name="fecha_ini"/>
              <field name="fecha_fin"/>
            </group>
            
            
          </sheet>

          <!--TRAZABILIDAD-->
          <div class="oe_chatter">
            <field name="message_follower_ids" widget="mail_followers"/>
            <field name="message_ids" widget="mail_thread" options="{'post_refresh': 'recipients'}"/>
          </div>


        </form>
      </field>
    </record>


    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="rhnominas.action_window1">
      <field name="name">nominas window</field>
