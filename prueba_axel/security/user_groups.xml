<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.module.category" id="module_management">
        <field name="name">Tipo de usuario</field>
        <field name="description">niveles de acceso de usuarios</field>
        <field name="sequence">3</field>
    </record>

    <record id="group_user" model="res.groups">
        <field name="name">Usuario</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
        <field name="category_id" ref="prueba_axel.module_management"/>
    </record>

    <record id="group_manager" model="res.groups">
        <field name="name">Administrador</field>
        <field name="implied_ids" eval="[(4, ref('prueba_axel.group_user'))]"/>
        <field name="category_id" ref="prueba_axel.module_management"/>
    </record>
</odoo>
