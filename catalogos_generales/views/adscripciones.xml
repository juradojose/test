<odoo>
  <data>
    <!--Search dependencia-->
    <record id="cat_generales_adscripcion_view_search" model="ir.ui.view">
          <field name="name">cat_generales.adscripciones.search</field>
          <field name="model">cat_generales.adscripciones</field>
          <field name="arch" type="xml">
              <search string="Search Patients">
                <field name="clave"/>
                <field name="nombre_ads"/>
                <field name="fecha_inicial"/>
                <field name="fecha_fin"/>
                <field name="dependencia"/>
                <field name="subdependencia"/>
                <field name="direccion"/>
                <field name="dir_area"/>
                <field name="jerarquia"/>
              </search>
          </field>
    </record>
    <!--Action adscripciones-->
        <record id="catalogos_adscripcion_action" model="ir.actions.act_window">
            <field name="name">Adscripciones Generales</field>
            <field name="res_model">cat_generales.adscripciones</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                    <p class="o_view_nocontent_smiling_face">Crear la primera adscipci√≥n.</p>
            </field>
        </record>
  <!--Adding custom submenus-->

        <menuitem
          id="catalogos_adscripcions_submenu"
          name="Adscipciones Generales"
          action="catalogos_adscripcion_action"
          parent="catalogo_nominas.catalogos_root_menu"
          sequence="10"
        />
  <!--end-->
    <!--Vista Adscripciones-->
    <record id="catalogos_adscipciones_view_form" model="ir.ui.view">
      <field name="name">cat_generales.adscripciones.view.form</field>
      <field name="model">cat_generales.adscripciones</field>
      <field name="arch" type="xml">
        <form string="Adscripciones generales">
          <sheet>
            <group>
              <field name="clave"/>
              <field name="nombre_ads"/>
              <field name="fecha_inicial"/>
              <field name="fecha_fin"/>
            </group>
            <group col="2">
              <field name="clave_dep"/>
              <field name="dependencia"/>
            </group>
            <group col="2">
              <field name="clave_sub"/>
              <field name="subdependencia"/>
            </group>
            <group col="2">
              <field name="clave_dir"/>
              <field name="direccion"/>
            </group>
            <group col="2">
              <field name="clave_area"/>
              <field name="dir_area"/>
            </group>
            <group col="2">
              <field name="clave_jer"/>
              <field name="jerarquia"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>
     <record id="catalogos_adscripciones_view_list" model="ir.ui.view">
      <field name="name">cat_generales.adscripciones.view.list</field>
      <field name="model">cat_generales.adscripciones</field>
      <field name="arch" type="xml" >
        <tree string="Adscripciones Generales">
              <field name="clave"/>
                <field name="nombre_ads"/>
                <field name="fecha_inicial"/>
                <field name="fecha_fin"/>
                <field name="dependencia"/>
                <field name="subdependencia"/>
                <field name="direccion"/>
                <field name="dir_area"/>
                <field name="jerarquia"/>
        </tree>
      </field>
    </record>
  <!-- fin vista dependencias-->
  </data>
</odoo>