<odoo>
  <data>
    <!--Search Adscripciones_particulares-->
    <record id="cat_generales_adscripcion_view_search" model="ir.ui.view">
          <field name="name">cat_generales.adscripciones_par.search</field>
          <field name="model">cat_generales.adscripciones_par</field>
          <field name="arch" type="xml">
              <search string="Search Patients">
                <field name="clave"/>
                <field name="nombre_ads_par" />
                <field name="fecha_inicial"/>
                <field name="fecha_fin"/>
                <field name="dependencia"/>
                <field name="subdependencia"/>
                <field name="direccion"/>
                <field name="dir_area"/>
                <field name="nombre_ads"/>
                <field name="jerarquia"/>
              </search>
          </field>
    </record>
    <!--Action adscripciones_par-->
        <record id="cat_generales_adscripcion_par_action" model="ir.actions.act_window">
            <field name="name">Adscripciones Particulares</field>
            <field name="res_model">cat_generales.adscripciones_par</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                    <p class="o_view_nocontent_smiling_face">Crear la primera adscipci√≥n.</p>
            </field>
        </record>
  <!--Adding custom submenus-->

        <menuitem
          id="cat_generales_adscripcions_submenu"
          name="Adscripciones Particulares"
          action="cat_generales_adscripcion_par_action"
          parent="catalogo_nominas.catalogos_root_menu"
          sequence="10"
        />
  <!--end-->
    <!--Vista adscripciones_par-->
    <record id="cat_generales_adscripciones_par_view_form" model="ir.ui.view">
      <field name="name">cat_generales.adscripciones_par.view.form</field>
      <field name="model">cat_generales.adscripciones_par</field>
      <field name="arch" type="xml">
        <form string="Adscripciones Particulares">
          <sheet>
            <group>
              <field name="clave"/>
              <field name="nombre_ads_par" />
              <field name="fecha_inicial"/>
              <field name="fecha_fin"/>
              <field name="dependencia"/>
              <field name="subdependencia"/>
              <field name="direccion"/>
              <field name="dir_area"/>
              <field name="nombre_ads"/>
              <field name="jerarquia"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>
     <record id="cat_generales_adscripciones_par_view_list" model="ir.ui.view">
      <field name="name">cat_generales.adscripciones_par.view.list</field>
      <field name="model">cat_generales.adscripciones_par</field>
      <field name="arch" type="xml" >
        <tree string="adscripciones_par Generales">
              <field name="clave"/>
              <field name="nombre_ads_par" />
              <field name="fecha_inicial"/>
              <field name="fecha_fin"/>
              <field name="dependencia"/>
              <field name="subdependencia"/>
              <field name="direccion"/>
              <field name="dir_area"/>
              <field name="nombre_ads"/>
              <field name="jerarquia"/>
        </tree>
      </field>
    </record>
  <!-- fin vista dependencias-->
  </data>
</odoo>